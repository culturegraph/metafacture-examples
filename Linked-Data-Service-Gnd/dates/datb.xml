<?xml version="1.0" encoding="UTF-8"?>
<rules xmlns="http://www.culturegraph.org/metamorph">
	<!-- 548 datb -->
	<combine name="${name}" value="${a}" reset="true" sameEntity="true">
		<data source="060R.4" name="name">
			<lookup>
				<entry name="datb" value="gnd:dateOfEstablishment" />
			</lookup>
		</data>
		<data source="060R.a" name="a" />
	</combine>
	<combine name="${name}" value="${b}" reset="true" sameEntity="true">
		<data source="060R.4" name="name">
			<lookup>
				<entry name="datb" value="gnd:dateOfTermination" />
			</lookup>
		</data>
		<data source="060R.b" name="b" />
	</combine>
	<combine name="${name}" value="${c}" reset="true" sameEntity="true">
		<data source="060R.4" name="name">
			<lookup>
				<entry name="datb" value="gnd:dateOfEstablishmentAndTermination" />
			</lookup>
		</data>
		<data source="060R.c" name="c" />
	</combine>
	<combine name="${name}" value="${d}" reset="true">
		<data source="060R.4" name="name">
			<lookup>
				<entry name="datb" value="gnd:dateOfEstablishmentAndTermination" />
			</lookup>
		</data>
		<data source="060R.d" name="d">
			<compose prefix="ca. " />
		</data>
	</combine>
</rules>